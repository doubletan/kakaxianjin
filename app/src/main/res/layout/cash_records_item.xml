<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="@dimen/length_5"
        android:background="@color/bg_color_ff"
        >
        <TextView
            android:id="@+id/cash_records_item_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/length_10"
            android:text="2018"
            android:textSize="@dimen/size_12"
            android:textColor="@color/text_color_66"
            android:layout_marginLeft="@dimen/length_10"
            android:lines="1"
            />
        <TextView
            android:id="@+id/cash_records_item_tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2018"
            android:textSize="@dimen/size_10"
            android:textColor="@color/text_color_66"
            android:layout_marginLeft="@dimen/length_10"
            android:lines="1"
            android:layout_below="@+id/cash_records_item_tv1"
            />
        <TextView
            android:id="@+id/cash_records_item_tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="审核中"
            android:textSize="@dimen/size_14"
            android:textColor="@color/text_color_66"
            android:layout_marginLeft="@dimen/length_10"
            android:layout_centerVertical="true"
            android:lines="1"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/length_10"
            />
    </RelativeLayout>

    <include layout="@layout/line_ef"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@color/bg_color_ff">
        
        <ImageView
            android:id="@+id/cash_records_item_iv1"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginLeft="@dimen/length_10"
            android:layout_centerVertical="true"
            />
        <TextView
            android:id="@+id/cash_records_item_tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="信用卡号"
            android:textSize="@dimen/size_12"
            android:textColor="@color/text_color_32"
            android:layout_toRightOf="@+id/cash_records_item_iv1"
            android:layout_marginTop="@dimen/length_10"
            android:lines="1"
            />
        <TextView
            android:id="@+id/cash_records_item_tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提款金额"
            android:textSize="@dimen/size_10"
            android:textColor="@color/text_color_66"
            android:layout_toRightOf="@+id/cash_records_item_iv1"
            android:lines="1"
            android:layout_below="@+id/cash_records_item_tv4"
            />
        <TextView
            android:id="@+id/cash_records_item_tv6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="卡号"
            android:textSize="@dimen/size_12"
            android:textColor="@color/text_color_32"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/length_10"
            android:layout_marginTop="@dimen/length_10"
            android:lines="1"
            />
        <TextView
            android:id="@+id/cash_records_item_tv7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2000元"
            android:textSize="@dimen/size_10"
            android:textColor="@color/text_color_66"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/length_10"
            android:lines="1"
            android:layout_below="@+id/cash_records_item_tv4"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_marginBottom="@dimen/length_5"
        android:background="@color/bg_color_ff">

        <ImageView
            android:id="@+id/cash_records_item_iv2"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginLeft="@dimen/length_10"
            android:layout_centerVertical="true"
            />
        <TextView
            android:id="@+id/cash_records_item_tv8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="收款卡号"
            android:textSize="@dimen/size_12"
            android:textColor="@color/text_color_32"
            android:layout_toRightOf="@+id/cash_records_item_iv2"
            android:layout_marginTop="@dimen/length_10"
            android:lines="1"
            />
        <TextView
            android:id="@+id/cash_records_item_tv9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="到账金额"
            android:textSize="@dimen/size_10"
            android:textColor="@color/text_color_66"
            android:layout_toRightOf="@+id/cash_records_item_iv2"
            android:lines="1"
            android:layout_below="@+id/cash_records_item_tv8"
            />
        <TextView
            android:id="@+id/cash_records_item_tv10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="卡号"
            android:textSize="@dimen/size_12"
            android:textColor="@color/text_color_32"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/length_10"
            android:layout_marginTop="@dimen/length_10"
            android:lines="1"
            />
        <TextView
            android:id="@+id/cash_records_item_tv11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2000元"
            android:textSize="@dimen/size_10"
            android:textColor="@color/text_color_66"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/length_10"
            android:lines="1"
            android:layout_below="@+id/cash_records_item_tv10"
            />
    </RelativeLayout>


</LinearLayout>
